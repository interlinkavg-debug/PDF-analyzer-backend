# Frontend URL for CORS
FRONTEND_URL=https://analysisai.vercel.app/

# Character chunk size for splitting large PDF text for summarization (default 3000)
SUMMARY_CHUNK_SIZE_CHARS=2000

# Timeout in seconds for external LLM API calls (default 30)
LLM_TIMEOUT_SECONDS=30

# Number of retry attempts on LLM API failures (default 3)
LLM_MAX_RETRIES=3

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO



